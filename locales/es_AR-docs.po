#: en[0]
msgid "Shapes"
msgstr "Figuras"

#: en[0].commands[0]
msgid "Draw a circle"
msgstr "Dibuja un círculo"

#: en[0].commands[0].args[0]
msgid "The circle size"
msgstr "Tamaño del círculo"

#: en[0].commands[1]
msgid "Draw an ellipse"
msgstr "Dibuja una elipse"

#: en[0].commands[1].args[0]
msgid "The ellipse width"
msgstr "Ancho de la elipse"

#: en[0].commands[1].args[1]
msgid "The ellipse height"
msgstr "Altura de la elipse"

#: en[0].commands[2]
msgid "Draw a square"
msgstr "Dibuja un cuadrado"

#: en[0].commands[2].args[0]
msgid "The square size"
msgstr "Tamaño del cuadrado"

#: en[0].commands[3]
msgid "Draw a rectangle"
msgstr "Dibuja un rectángulo"

#: en[0].commands[3].args[0]
msgid "The rectangle width"
msgstr "Ancho del rectángulo"

#: en[0].commands[3].args[1]
msgid "The rectangle height"
msgstr "Altura del rectángulo"

#: en[0].commands[4]
msgid "Draw part of a circle"
msgstr "Dibuja parte de un círculo"

#: en[0].commands[4].args[0]
msgid "The size"
msgstr "Tamaño"

#: en[0].commands[4].args[1]
msgid "The start point (between 0 to 2)"
msgstr "El punto de inicio (entre 0 y 2)"

#: en[0].commands[4].args[2]
msgid "The end point (between 0 to 2)"
msgstr "El punto de fin (entre 0 y 2)"

#: en[0].commands[4].args[3]
msgid "Close the shape"
msgstr "Cierra la figura"

#: en[0].commands[5]
msgid "Draw a many sided shape"
msgstr "Dibuja una figura de muchos lados"

#: en[0].commands[5].args[0]
msgid "Point X position"
msgstr "Indica la posición X"

#: en[0].commands[5].args[1]
msgid "Point Y position"
msgstr "Indica la posición Y"

#: en[0].commands[5].args[2]
msgid "More polygon points positions"
msgstr "Más puntos de posición del polígono"

#: en[0].commands[5].args[3]
msgid "Close the path in the middle"
msgstr "Cierra la figura en el medio"

#: en[1]
msgid "Lines"
msgstr "Líneas"

#: en[1].commands[0]
msgid "Draw a line of a certain size"
msgstr "Dibuja una línea de un determinado tamaño"

#: en[1].commands[0].args[0] en[2].commands[0].args[0]
msgid "Horizontal distance"
msgstr "Distancia horizontal"

#: en[1].commands[0].args[1] en[2].commands[0].args[1]
msgid "Vertical distance"
msgstr "Distancia vertical"

#: en[1].commands[1]
msgid "Draw a line to a certain point"
msgstr "Dibuja una línea hasta un determinado punto"

#: en[1].commands[1].args[0] en[2].commands[1].args[0]
msgid "The horizontal destination"
msgstr "El punto de destino horizontal"

#: en[1].commands[1].args[1] en[2].commands[1].args[1]
msgid "The vertical destination"
msgstr "El punto de destino vertical"

#: en[2]
msgid "Position"
msgstr "Posición"

#: en[2].commands[0]
msgid "Move the cursor a certain distance away"
msgstr "Mueve el cursor una determinada distancia"

#: en[2].commands[1]
msgid "Move the cursor to a particular position"
msgstr "Mueve el cursor a una determinada posición"

#: en[3]
msgid "Text"
msgstr "Texto"

#: en[3].commands[0]
msgid "Write text"
msgstr "Escribe texto"

#: en[3].commands[0].args[0]
msgid "The message to write"
msgstr "El mensaje a escribir"

#: en[3].commands[1]
msgid "Set size and/or font"
msgstr "Configura el tamaño y/o la fuente"

#: en[3].commands[1].args[0]
msgid "The font family name"
msgstr "El nombre de la fuente"

#: en[3].commands[1].args[1]
msgid "The text size in pixels"
msgstr "El tamaño del texto en píxeles"

#: en[3].commands[2]
msgid "Sets bold text on (true) or off (false)"
msgstr "Activa la negrita (verdadero) o desactívala (falso)"

#: en[3].commands[2].args[0]
msgid "Bold state (True by default)"
msgstr "Estado de la negrita (Verdadero es el predeterminado)"

#: en[3].commands[3]
msgid "Sets italic text on (true) or off (false)"
msgstr "Activa la cursiva (verdadero) o desactívala (falso)"

#: en[3].commands[3].args[0]
msgid "Italic state (True by default)"
msgstr "Estado de la cursiva (Verdadero es el predeterminado)"

#: en[4]
msgid "General"
msgstr "General"

#: en[4].commands[0]
msgid "Repeat code"
msgstr "Repetir código"

#: en[4].commands[0].args[0]
msgid "The start and end point for the variable i"
msgstr "Punto de inicio y fin de la variable i"

#: en[4].commands[1]
msgid "Get a random number in a range."
msgstr "Obtén un número aleatorio dentro de un rango."

#: en[4].commands[1].args[0]
msgid "The minimum value"
msgstr "El valor mínimo"

#: en[4].commands[1].args[1]
msgid "The maximum value"
msgstr "El valor máximo"

#: en[4].commands[1].args[2]
msgid "Set to true to return decimal values"
msgstr "Configura a verdadero para recibir valores decimales"

#: en[5]
msgid "Colors"
msgstr "Colores"

#: en[5].commands[0]
msgid "Set the background color"
msgstr "Configura el color del fondo"

#: en[5].commands[0].args[0]
msgid "The background color to set"
msgstr "El color del fondo a configurar"

#: en[5].commands[1]
msgid "Change the color in use"
msgstr "Cambia el color en uso"

#: en[5].commands[1].args[0]
msgid "The object color to set"
msgstr "El color del objeto a configurar"

#: en[5].commands[2]
msgid "Change the width and color of the stroke (border)"
msgstr "Cambia el color y el ancho del trazo (borde)"

#: en[5].commands[2].args[0]
msgid "A string with the color to use - E.g. 'red', 'blue'.."
msgstr "El color a usar - Ej. 'red', 'blue'.."

#: en[5].commands[2].args[1]
msgid "(number) The width to use"
msgstr "(número) El ancho a usar"

#: en[5].commands[3]
msgid "Set a colour's brightness"
msgstr "Configura el brillo del color"

#: en[5].commands[3].args[0] en[5].commands[4].args[0]
msgid "Color"
msgstr "Color"

#: en[5].commands[3].args[1]
msgid "amount of brightness to set (-100 to 100)"
msgstr "monto del brillo a configurar (-100 a 100)"

#: en[5].commands[4]
msgid "Set a colour's saturation"
msgstr "Configura la saturación del color"

#: en[5].commands[4].args[1]
msgid "amount of saturation to set (-100 to 100)"
msgstr "monto de saturación a configurar (-100 a 100)"

#: en[5].commands[5]
msgid "Rotate a color's hue angle by given amount"
msgstr "Cambia el tono de un color en base a un determinado valor"

#: en[5].commands[5].args[0]
msgid "Color to rotate"
msgstr "Color a cambiar"

#: en[5].commands[5].args[1]
msgid "angle to rotate (-360 - 360)"
msgstr "tono a configurar (-360 - 360)"

#: en[5].commands[6]
msgid "Set how see through a color is"
msgstr "Configura cómo se ve a través de un color"

#: en[5].commands[6].args[0]
msgid "Color to saturate"
msgstr "Color a saturar"

#: en[5].commands[6].args[1]
msgid "amount of opacity to subtract (-100 to 100)"
msgstr "monto de opacidad (-100 a 100)"
